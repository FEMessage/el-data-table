{
  "name": "el-data-table",
  "version": "1.2.2",
  "description": "base on element-ui, makes crud easily",
  "author": "https://github.com/FEMessage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FEMessage/el-data-table.git"
  },
  "keywords": ["vue", "element-ui", "table", "data-table", "element table"],
  "files": ["src", "dist", "yarn.lock"],
  "main": "dist/el-data-table.umd.js",
  "module": "dist/el-data-table.esm.js",
  "unpkg": "dist/el-data-table.min.js",
  "browser": {
    "./sfc": "src/el-data-table.vue"
  },
  "scripts": {
    "dev": "vue-styleguidist server",
    "doc": "vue-styleguidist build",
    "build":
      "npm run build:unpkg & npm run build:es & npm run build:umd & npm run doc",
    "build:umd":
      "rollup --config build/rollup.config.js --format umd --file dist/el-data-table.umd.js",
    "build:es":
      "rollup --config build/rollup.config.js --format es --file dist/el-data-table.esm.js",
    "build:unpkg":
      "rollup --config build/rollup.config.js --format iife --file dist/el-data-table.min.js",
    "precommit": "pretty-quick --staged",
    "stdver": "standard-version -m '[skip ci] chore(release): v%s'",
    "release": "gren release --override"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "qs": "^6.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@femessage/el-form-renderer": "^1.5.4",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "element-ui": "^2.4.11",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "husky": "^0.14.3",
    "minimist": "^1.2.0",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.16",
    "vue-loader": "^15.7.0",
    "vue-styleguidist": "^3.11.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.29.6"
  },
  "vue-sfc-cli": "1.6.0",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
